    "electron": "^34.0.1",
    "electron-builder": "^25.1.8",
    "eslint": "^9.19.0",
    "lint-staged": "^15.4.3",
    "rimraf": "^6.0.1",
    "simple-git-hooks": "^2.11.1",
    "typescript": "^5.7.3",
    "vite": "^6.0.11",
